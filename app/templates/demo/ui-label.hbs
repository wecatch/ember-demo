{{outlet}}

<div class="ui container" >
    <div class="ui dividing header" >ui label</div>
    <div class="ui one column grid" >
        <div class="row" >
            <div class="column" >
                <div class="ui label" >i am label</div>
                <div class="ui basic label" >i am label</div>
                <div class="ui blue image label" >
                    i am label
                    <div class="detail" >detail</div>
                </div>
                <div class="ui pointing label">i am label</div>
                <div class="ui red left pointing label" > i am label</div>
                <div class="ui orange right pointing label">i am label</div>
                <div class="ui blue below pointing label">i am label</div>
                <div class="ui tag blue below pointing label">i am label</div>
                <div>
                    {{#ui-label theme="raised segment" }}
                        <div class="ribbon" >i am label</div>
                    {{/ui-label}}
                </div>

                <div>
                    {{#ui-label}}label{{/ui-label}}
                    {{#ui-pointing-label direction="below" }}
                        below label
                    {{/ui-pointing-label}}

                    {{#ui-pointing-label direction="left" }}
                        left label
                    {{/ui-pointing-label}}

                    {{#ui-pointing-label direction="right" }}
                        right lbale
                    {{/ui-pointing-label}}
                </div>

                <div>
                    {{#ui-label theme="basic" }}label{{/ui-label}}
                    {{#ui-label theme="basic red" }}label{{/ui-label}}
                    {{#ui-label theme="red" }}label{{/ui-label}}
                    {{#ui-label  }}label{{/ui-label}}
                    {{#ui-label theme="red image" }}
                        <img src="https://www.baidu.com/img/bd_logo1.png">
                        baidu
                    {{/ui-label}}
                    {{#ui-label theme="basic" }}label{{/ui-label}}
                    {{#ui-label theme="basic" }}label{{/ui-label}}
                    {{#ui-label theme="basic" }}label{{/ui-label}}

                </div>
            </div>
        </div>
    </div>
</div>